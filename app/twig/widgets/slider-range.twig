<div class="slider-list mb20">
    <div class="slider-block">
        <label for="slider-range" class="lh18 mb10 fz15 f-normal">Сумма займа
            (тенге)</label>
        <p type="text" id="sum-value" class=" fz20 text-black">
        <input class="slider-range" type="text" data-slider-min="200000"
               data-slider-max="12000000" data-slider-step="1000"
               data-slider-value="3"/>
        <div class="clearfix">
            <span class="slider-val-min left">200 000</span>
            <span class="slider-val-max right">12 000 000</span>
        </div>
    </div>
    <div class="slider-block">
        <label for="slider-range" class="lh18 mb10 fz15 f-normal">Срок займа</label>
        <input type="text" id="time-val">
        <input class="slider-range" type="text" data-slider-max="180"
               data-slider-step="10"
               data-slider-value="10"/>
        <div class="clearfix">
            {#
                                                <span  class="slider-val-min left">200000</span>
            #}
            <span class="slider-val-max right">180 дней</span>
        </div>
    </div>
</div>